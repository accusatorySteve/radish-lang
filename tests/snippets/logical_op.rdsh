// Test logical operators.

var a = true and true
print a // expect: true

var b = true and false
print b // expect: false

var c = false and nil + 5
print c // expect: false

var d = false or true
print d // expect: true

var e = false or false 
print e // expect: false

var f = true or nil + 34
print f // expect: true

var g = 2 > 1 and 3 == 3 or 5 <= 2 or true and nil == nil
print g // expect: true 