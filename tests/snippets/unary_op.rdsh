// Test unary operators.

print -2 // expect: -2
print -2 + -10 // expect: -12
print -2 + 3 // expect: 1
print -(2 + 3) // expect: -5
print -5 - -4 // expect: -1
print -2 * -10 // expect: 20
print -----2 // expect: -2
print -456987 // expect: -456987

print !true // expect: false
print !false // expect: true
print !!true // expect: true