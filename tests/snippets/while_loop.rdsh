// Test while loops

var i = 0

while i < 10 loop
    i = i + 1
endloop

print i // expect: 10

i = 0

while i < 10 loop
    if i == 5 then
        break
    end

    i = i + 1
endloop

print i // expect: 5

i = 0
var c = 0

while i < 10 loop
    i = i + 1

    if i == 5 then
        c = c + 6
        continue
    end

    c = c + 1
endloop

print c // expect: 15