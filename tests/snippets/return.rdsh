// Test return statements

fun add(a, b) {
    return a + b
}

print add(1, 4) // expect: 5

// weird function to test returning from loops
fun sum(num) {
    var c = 0
    var result = 0
    while c != num loop
        result += c
        c += 1
        if c == 10 then
            return result
        endif
    endloop

    return result
} 

print sum(16) // expect: 45